{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "decorators": true
        },
        "baseUrl": "./src",
        "keepClassNames": true,
        "paths": {
            "#seedwork/*": [
                "./@seedwork/*"
            ],
            "#category/*": [
                "./category/*"
            ],
            "#cast-member/*": [
                "./cast-member/*"
            ],
            "#genre/*": [
                "./genre/*"
            ]
        }
    }
}